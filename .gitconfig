[user]
    email = hchen.robotics@gmail.com
    name = Harrison Chen
[alias]
    a = add
    c = commit
    ca = commit --amend
    co = checkout
    f = fetch
    ps = push
    psf = push --force-with-lease
    pl = pull
    s = status
    sw = switch

    dag = log --branches --remotes --tags --graph --decorate --oneline
    stash-all = stash --include-untracked
    uncommit = reset --soft HEAD~1
    unstage = restore --staged
[commit]
    template = ~/.gitmessage.txt
    verbose = true
[core]
    editor = code --wait
    excludesFile = ~/.gitignore_global
[fetch]
    prune = true
[help]
    autocorrect = true
[init]
    defaultBranch = main
[pull]
    ff = only
[status]
    branch = true
    short = true
